{
  "name": "vue-rank-slider",
  "version": "0.1.1",
  "description": "A component for vue, get rank value by slide",
  "keywords": [
    "vue",
    "component",
    "rank",
    "slider",
    "rank slider"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Anluis/vue-rank-slider"
  },
  "author": "Anluis",
  "license": "MIT",
  "main": "dist/vue-rank-slider.umd.js",
  "module": "dist/vue-rank-slider.esm.js",
  "unpkg": "dist/vue-rank-slider.min.js",
  "browser": {
    "./sfc": "src/RankSlider.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:umd & yarn build:es & yarn build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-rank-slider.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-rank-slider.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-rank-slider.min.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "rollup": "^2.0.2",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue-template-compiler": "^2.6.11"
  }
}
